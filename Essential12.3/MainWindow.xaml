<Window x:Class="Essential12._3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Essential12._3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="StartS" Content="Start" HorizontalAlignment="Left" Margin="92,45,0,0" VerticalAlignment="Top" Width="75" Click="StartS_Click"/>
        <Button x:Name="StopS" Content="Stop" HorizontalAlignment="Left" Margin="92,81,0,0" VerticalAlignment="Top" Width="75" Click="StopS_Click"/>
        <Button x:Name="ClearS" Content="Clear" HorizontalAlignment="Left" Margin="92,118,0,0" VerticalAlignment="Top" Width="75" Click="ClearS_Click"/>
        <TextBox x:Name="SecondBox" HorizontalAlignment="Left" Height="23" Margin="231,81,0,0" TextWrapping="Wrap" Text="Seconds" VerticalAlignment="Top" Width="125"/>

    </Grid>
</Window>
